@model List<UserToReturnViewModel>
@{
    ViewData["Title"] = "Home Page";
    ViewBag.IsLoggedout = true;
}

<Partial name="_NavBarPartial"/>
<div class="container">
        <main role="main" class="pb-3">
   <div class="my_card-panel">
       @foreach(var user in Model)
                {
                  <a href="#">
                <div class="my_card">
                   <div class="my_card_image_box">
                       <img scr="@user.PhotoUrl"/>
                   </div>
                   <div class="my_card-desc">
                       <p>
                                    @user.FirstName @user.LastName
                       </p>
                       <p>
                                    @user.Email
                       </p>
                   </div>
               </div>
                  </a>     
                }
            </div>
        </main>
    </div>